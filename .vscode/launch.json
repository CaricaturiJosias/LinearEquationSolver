{
    "version": "0.2",
    "configurations": [
      {
        "name": "Debug C++ by Executable Name",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/solver", // Replace with your executable name
        "args": [], // Command line arguments if needed
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "miDebuggerPath": "/usr/bin/gdb" // Modify the path to gdb if needed
      }
    ]
  }
  